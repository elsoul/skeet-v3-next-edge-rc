  "engines": {
    "pnpm": ">=9",
    "npm": "please_use_pnpm_instead",
    "node": ">=20"
  },
  "packageManager": "pnpm@9.1.4",
  "scripts": {
    "dev": "next dev -p 4200",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "pnpm next-on-pages",
    "preview": "pnpm pages:build && wrangler pages dev .vercel/output/static --d1 SKEET_AUTH_DB=b27d329d-7116-4716-9e7d-dbacb9a92ec1",
    "deploy": "pnpm pages:build && wrangler pages deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "npm:update": "pnpm update \"!eslint\" --latest",
    "migration:create": "wrangler d1 migrations create skeet-auth-database",
    "migration:init": "prisma migrate diff --from-empty --to-schema-datamodel ./prisma/auth.prisma --script",
    "migration:evolve": "prisma migrate diff --from-local-d1 --to-schema-datamodel ./prisma/auth.prisma --script",
    "migration:apply": "wrangler d1 migrations apply skeet-auth-database",
    "migration:generate": "prisma generate --schema=./prisma/auth.prisma",
    "ui:add": "pnpm dlx shadcn-ui@latest add"
  },
  "dependencies": {
    "@auth/prisma-adapter": "2.1.0",
    "@hookform/resolvers": "3.4.2",
    "@prisma/adapter-d1": "5.14.0",
    "@prisma/client": "5.14.0",
    "@radix-ui/react-avatar": "1.0.4",
    "@radix-ui/react-dialog": "1.0.5",
    "@radix-ui/react-dropdown-menu": "2.0.6",
    "@radix-ui/react-icons": "1.3.0",
